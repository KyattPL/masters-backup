@misc{chatgpt_narrative,
  author   = {Umbraško, Arturs and Drury, Robert},
  url      = {https://hdl.handle.net/2077/78909},
  title    = {Applying ChatGPT in AI-based dynamic video game narrative generation system},
  year     = {2023},
  abstract = {ChatGPT’s release and general availability has the potential to create new possibilities for storytelling in games. There are potential benefits of leveraging ChatGPT to generate a narrative at runtime in a video game, as well as possible resulting pitfalls. To explore these factors, we created a game prototype that dynamically instantiates game assets from JSON, generated by ChatGPT. We additionally incorporate a dynamic status-effect system and, in conjunction with textual player input, utilise this in order to instruct ChatGPT to generate an informed narrative as the game is played. Player feedback indicates that ChatGPT’s addition is beneficial overall, though not without some downsides.}
}

