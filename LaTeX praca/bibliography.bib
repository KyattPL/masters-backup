@inproceedings{questville,
  author    = {Al-Nassar, Suzan and Schaap, Anthonie and Zwart, Michael Van Der and Preuss, Mike and G\'{o}mez-Maureira, Marcello A.},
  title     = {QuestVille: Procedural Quest Generation Using NLP Models},
  year      = {2023},
  isbn      = {9781450398558},
